<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles.css">
    <title>Posts</title>
</head>
<body>
    <%- include("navbar.ejs") %>
    <br>
    <br>
    <div class="tweet">
        <% tweets.forEach(item=>{ %>
            <a href="/user/<%= item.author._id%>">
                <img src="/images/<%= item.author.pfImage%>">
                <em><%= item.author.username%></em>
            </a>
            <%= item.publishingDate%>
            <br>
            <h3><%= item.story%></h3>
            <hr>
        <% }) %>
    </div>
</body>
</html>